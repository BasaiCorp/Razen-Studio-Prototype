# Razen Syntax Test File - V3

# Structs and Type Annotations
struct userData {
    name<str>,
    age<num>
}

# Variable and Constant Declarations
var<num> x = 10
const<str> app_name = "Razen Studio"
var any_type_value = "Prathmesh"

# Library Usage
use math
use "discordRZN"

# Namespace operator
var<num> squared = math::sqrt(256)

# F-string and show<color>
show<green> f"The square root of 256 is {squared}."
show<light_red> "This is a test message."

# Control Flow
match squared {
    16: show "Correct!",
    default: show "Incorrect."
}

# Standalone types should not be highlighted
num str bool map list arr obj tuple

# A regular function call for comparison
some_function()
